{
  "name": "rest-express",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cross-env NODE_ENV=development PORT=5002 tsx --require dotenv/config server/index.ts",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build:production",
    "build:server": "cd server && npm install && cd .. && tsc --project server/tsconfig.json",
    "build:admin": "cd client && npm run build:production",
    "start": "cross-env NODE_ENV=production tsx server/index.ts",
    "preview": "npm run build && npm run start",
    "deploy": "node scripts/deploy.js",
    "deploy:firebase": "npm run build:admin && firebase deploy",
    "deploy:hosting": "npm run build:admin && firebase deploy --only hosting:admin-app",
    "deploy:functions": "npm run build:server && firebase deploy --only functions",
    "firebase:setup-targets": "node scripts/setup-targets.js",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
